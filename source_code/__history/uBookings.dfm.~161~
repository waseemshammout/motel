  object dsBookingGuests: TFDQuery
    Connection = dm.con
    SQL.Strings = (
      
        'select bgd.id, b.booking_id, g.guest_id, g.first_name, g.last_na' +
        'me, g.phone, g.email, g.date_of_birth, g.birth_country, g.id_num' +
        'ber, g.id_type, g.guardian_id'
      'from booking_guests_details bgd'
      'join bookings b on bgd.booking_id = b.booking_id'
      'join guests g on bgd.guest_id = g.guest_id'
      'where b.booking_id = :pBookingID')
    Left = 681
    Top = 436
    ParamData = <
      item
        Name = 'PBOOKINGID'
        ParamType = ptInput
      end>
  end
  object cmd: TFDCommand
    Connection = dm.con
    CommandKind = skOther
    Left = 744
    Top = 432
